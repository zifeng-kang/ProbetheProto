{
  "name": "Crawler Extension",
  "description": "This is an extension that crawls links from a starting domain",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Crawler Extension"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["queue.js", "background.js"]
  }
}
